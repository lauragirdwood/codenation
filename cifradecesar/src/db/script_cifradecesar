CREATE DATABASE cifradecesar;
USE DATABASE cifradecesar;

CREATE TABLE resposta (
    id INTEGER NOT NULL AUTO_INCREMENT,
    numero_casas INTEGER,
    token VARCHAR(255),
    cifrado VARCHAR(255),
    decifrado VARCHAR(255),
    resumo_criptografico VARCHAR(255),
    PRIMARY KEY (id)
);

